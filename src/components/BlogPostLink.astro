---
import {
    Card,
    CardContent,
    CardDescription,
    CardFooter,
    CardHeader,
    CardTitle,
} from "@/components/ui/card";
const { title, url, desc } = Astro.props;
---

<Card>
    <CardHeader>
        <CardTitle>
            {title}
        </CardTitle>
    </CardHeader>
    <CardContent> <p>{desc}</p> </CardContent>
    <CardContent> <a href={url}>Link</a> </CardContent>
</Card>
